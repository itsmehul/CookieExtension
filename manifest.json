{
    "manifest_version": 2,
    "name": "Cookie creator",
    "version": "0.1",
    "browser_action": {
        "default_icon": "cookie.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.blockchainsemantics.com/",
                "https://www.blockchainsemantics.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}