{
    "manifest_version": 2,
    "name": "CookieCreator",
    "version": "0.1",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.blockchainsemantics.com/",
                "https://www.blockchainsemantics.com/*"
            ],
            "js": [
                "jq.min.js",
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "cookie.png"
    }
}